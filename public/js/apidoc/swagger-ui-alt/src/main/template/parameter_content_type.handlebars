<div>
    <label for="parameterContentType">Content type:</label>
    <select class="parameter form-control" name="parameterContentType">
        {{#if consumes}}
            {{#each consumes}}
                <option value="{{{this}}}">{{{this}}}</option>
            {{/each}}
        {{else}}
            <option value="application/json">application/json</option>
        {{/if}}
    </select>
</div>
