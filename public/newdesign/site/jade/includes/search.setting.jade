				a( href="#dataprovider" title="" data-toggle="tab") DATA PROVIDER
			li
				a( href="#creator" title="" data-toggle="tab") CREATOR
			li
				a( href="#right" title="" data-toggle="tab") RIGHTS
			li
				a( href="#country" title="" data-toggle="tab") COUNTRY
			li
				a( href="#year" title="" data-toggle="tab") YEAR

