// settings
.searchsetting

	// filter Text
	.filtertext
		input.filterinput( type="text" placeholder="Search" )
		img.filterimg( alt="search" src="img/ui/ic-mag.png" )
		

	// wrap
	.wraps

		// tabs
		ul.nav.nav-tabs( role="tablist" )
			li.active
				a( href="#sources" title="" data-toggle="tab" ) SOURCES
			li
				a( href="#type" title="" data-toggle="tab" ) TYPE
			li
				a( href="#provider" title="" data-toggle="tab" ) PROVIDER
